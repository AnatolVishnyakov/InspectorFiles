<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootLayout" prefHeight="670.0" prefWidth="1080.0" styleClass="window-root" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.inspector_files.controller.RootController">
    <Pane prefHeight="670.0" prefWidth="1080.0" styleClass="window-root" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171">
        <children>
            <BorderPane fx:id="contentPanel" layoutY="59.0" prefHeight="558.0" prefWidth="1080.0" />
            <HBox layoutX="268.0" prefHeight="60.0" prefWidth="812.0" styleClass="navigation-horizontal-box" stylesheets="@../css/style.css">
                <children>
                    <JFXButton id="dashboardButton" mnemonicParsing="false" onAction="#onDashboardButtonClick" prefHeight="59.0" prefWidth="164.0" text="Dashboard">
                        <font>
                            <Font size="14.0" />
                        </font>
                    </JFXButton>
                    <JFXButton id="revisorButton" mnemonicParsing="false" onAction="#onSnapshotPanel" prefHeight="59.0" prefWidth="164.0" text="Ревизор">
                        <font>
                            <Font size="14.0" />
                        </font>
                    </JFXButton>
                </children>
            </HBox>
         <JFXButton fx:id="buttonMinimize" layoutX="27.0" layoutY="-5.0" text="_" textFill="WHITE" />
         <JFXButton fx:id="buttonClose" layoutX="-1.0" layoutY="-3.0" text="X" textFill="RED" />
        </children>
    </Pane>
</AnchorPane>
