<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="rootLayout" prefHeight="670.0" prefWidth="1080.0" styleClass="window-root"
            stylesheets="@../css/style.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.inspector_files.controller.RootController">
    <Pane prefHeight="670.0" prefWidth="1080.0" styleClass="window-root" stylesheets="@../css/style.css"
          xmlns="http://javafx.com/javafx/8.0.171">
        <children>
            <BorderPane fx:id="contentPanel" layoutY="59.0" prefHeight="558.0" prefWidth="1080.0"/>
            <HBox layoutX="268.0" prefHeight="60.0" prefWidth="812.0" styleClass="navigation-horizontal-box"
                  stylesheets="@../css/style.css">
                <children>
                    <JFXButton id="dashboardButton" mnemonicParsing="false" prefHeight="59.0" prefWidth="164.0"
                               text="Dashboard" onAction="#onDashboardButtonClick">
                        <font>
                            <Font size="14.0"/>
                        </font>
                    </JFXButton>
                    <JFXButton id="revisorButton" mnemonicParsing="false" prefHeight="59.0" prefWidth="164.0"
                               text="Ревизор" onAction="#onRevisorButtonClick">
                        <font>
                            <Font size="14.0"/>
                        </font>
                    </JFXButton>
                </children>
            </HBox>
        </children>
    </Pane>
</AnchorPane>
